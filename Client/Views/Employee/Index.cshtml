@{
    ViewData["Title"] = "Dashboard";
    Layout = "_LayoutAdmin";
}

<h2 class="text-center">Employee List</h2>
<div class="table-responsive mx-auto">
    <table class="table table-striped table-hover table-primary" id="tbodyEmployee">
        <thead class="table-dark">
            <tr>
                <th>No</th>
                <th>Nik</th>
                <th>FullName</th>
                <th>Birth Date</th>
                <th>Gender</th>
                <th>Hiring Date</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Major</th>
                <th>Degree</th>
                <th>GPA</th>
                <th>University</th>
                <th class="col-4">Action</th>
            </tr>
        </thead>
        <tbody>
        </tbody>
    </table>
</div>


@section scripts {

    <script src="~/js/EmployeeDetails.js" asp-append-version="true"></script>

}