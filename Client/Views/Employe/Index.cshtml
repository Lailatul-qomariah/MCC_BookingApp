@{
    ViewData["Title"] = "Dashboard";
    Layout = "_LayoutAdmin";
}
<div class="container">
    <h2 class="text-center">Employee List</h2>
    <div class="table-responsive mx-auto">
        <table class="table table-striped table-hover table-primary" id="tbodyEmployee">
            <thead class="table-dark">
                <tr>
                    <th>#</th>
                    <th>Nik</th>
                    <th>FullName</th>
                    <th>Hiring Date</th>
                    <th>Email</th>
                    <th>PhoneNumber</th>
                    <th>Major</th>
                    <th>Degree</th>
                    <th>GPA</th>
                    <th>University</th>
                </tr>
            </thead>
            <tbody>
                <!-- Pokémon data will be inserted here by jQuery -->
            </tbody>
        </table>
    </div>
</div>

@section scripts {
    
    <script src="~/js/Employee.js" asp-append-version="true"></script>

}